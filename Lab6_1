#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
#define _USE_MATH_DEFINES

int main() {
	setlocale(LC_ALL, "RUS");
	int year;
	puts("Введите год\n");
	scanf_s("%i", &year);
	if (((year % 4 == 0) & (year % 100 != 0)) || year % 400 == 0) printf("Год %i високосный", year);
	else printf("Год %i не високосный", year);
}
